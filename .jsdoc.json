  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": "./src/index.js",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "private": false,
    "template": "node_modules/docdash"
  }
}
